{
  "name": "@automation/m4nodejs",
  "version": "1.1.12",
  "description": "M4JSAPI inside Node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "concat-stream": "^2.0.0",
    "rxjs": "^6.5.4",
    "tough-cookie": "^3.0.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "window": "^4.2.6",
    "@types/jest": "^24.9.1",
    "@types/jsdom": "^12.2.4",
    "@types/node": "^13.1.8",
    "@types/tough-cookie": "^2.3.6",
    "mock-xmlhttprequest": "^5.1.0"
  },
  "devDependencies": {
    "jest": "^25.1.0",
    "ts-jest": "^25.0.0",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "deploy": "tsc && npm publish",
    "exampleDebugCookie": "ts-node-dev --respawn --transpileOnly ./src/_examples/app.debug.cookies.ts",
    "exampleDisplayLogMsg": "ts-node-dev --respawn --transpileOnly ./src/_examples/app.display.logMessages.ts",
    "exampleMock": "ts-node-dev --respawn --transpileOnly ./src/_examples/app.mock.ts",
    "exampleRxjsObservable": "ts-node-dev --respawn --transpileOnly ./src/_examples/app.rxjs.observables.ts",
    "exampleWithConstructor": "ts-node-dev --respawn --transpileOnly ./src/_examples/app.use.m4nodejs.constructor.ts",
    "exampleWithFactory": "ts-node-dev --respawn --transpileOnly ./src/_examples/app.use.m4nodejs.factory.ts",
    "exampleWithWindowObject": "ts-node-dev --respawn --transpileOnly ./src/_examples/app.use.windowObject.ts",
    "exampleDebugCookieDist": "tsc && node ./dist/_examples/app.debug.cookies.js",
    "createXmlMetatadata": "ts-node-dev --respawn --transpileOnly ./src/_examples/app.testutils.ts"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:franjmr/m4Node.js.git"
  },
  "keywords": [
    "m4nodejs"
  ],
  "author": "Automation",
  "license": "ISC",
  "publishConfig": {
    "registry": "http://nexus.meta4.com/repository/m4npm/"
  }
}
